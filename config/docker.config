               {crlfile_path, "/opt/ca/crl/crl.pem"}]},
  {lager, [
    {handlers, [
               {lager_console_backend, [{level, debug}]}
               %% {lager_syslog_backend, ["epp_proxy", local1, info]}
    ]}
 ]}
